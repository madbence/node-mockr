{
  "name": "mockr",
  "version": "0.1.0",
  "description": "Simple mock framework",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha -R spec test/*",
    "cover": "istanbul cover _mocha -- -R spec test/*",
    "coveralls": "istanbul cover _mocha --report lcovonly -- -R spec test/* && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/madbence/node-mockr.git"
  },
  "keywords": [
    "mock",
    "test",
    "mocha",
    "unit",
    "framework"
  ],
  "author": "Bence DÃ¡nyi <madbence@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/madbence/node-mockr/issues"
  },
  "devDependencies": {
    "mocha": "~1.16.2",
    "should": "~2.1.1",
    "istanbul": "~0.2.3",
    "coveralls": "~2.6.1"
  },
  "dependencies": {
    "mock-fun": "~1.1.2"
  }
}
